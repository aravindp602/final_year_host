{
  "name": "automl-tool",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "react-start": "PORT=3001 react-scripts start",
    "dev": "concurrently \"npm run react-start\" \"npm start\"",
    "react-build": "react-scripts build",
    "test": "react-scripts test",
    "electron-build": "electron-builder"
  },
  "build": {
    "appId": "com.yourname.automltool",
    "productName": "AutoML Tool",
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "main.js",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.13.2",
    "electron": "^31.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "reactflow": "^11.11.4",
    "recharts": "^3.4.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "electron-builder": "^26.0.12"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
